package com.sathya.Array1;

import java.util.Iterator;

public class TestEx {
public static void main(String[] args) {
	int[] arr= {1,2,3,4,5,4,5,6,6};
	for (int i = 0; i < arr.length; i++) {
		int freq=1;
		for (int j =i+1; j < arr.length; j++) {
			if(arr[j]==arr[i]) {
				freq++;
			System.out.println(arr[i]+" --->"+freq);
			}
			else
			{
				System.out.println(arr[i]+" --->"+freq);
			}
		}
	}
}
}