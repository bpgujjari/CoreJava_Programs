package com.sathya.vi;

public class Test {
public static void main(String[] args) {
	int[] arr= {3,4,6,5,7,8};
	for (int i = 0; i < arr.length; i++) {
		boolean isprime=true;
		for (int j = 2; j <arr[i]; j++) {
			if(arr[i]%j==0)
			{
				isprime=false;
				break;
			}
			
		}
		if(isprime)
		{
			System.out.println(arr[i]);
		}
	}
}
}
